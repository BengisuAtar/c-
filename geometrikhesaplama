Console.Write("Sayı Giriniz:");//sayıları kuklanıcıdan dinamik olarak aldım
double sayi1= Convert.ToDouble(Console.ReadLine());

Console.Write("Sayı Giriniz:");//writeline kod satırı consolda alt satıra geçtigi için write kullandım(write kodu alt satıra geçmiyor)
double sayi2 = Convert.ToDouble(Console.ReadLine());//sayıları ondalıklı alma sebebim sonucu kesin ve doğru görebilmektir

double hesaplama=Math.Sqrt(sayi1*sayi2);// math.sqrt metodu kullanma sebebim sayıların karekökünü almak(geometrik hesaplamasını yapmak için )

Console.Write("Cevap="+hesaplama);//buldugum sonucu ekrana yazdım

Console.ReadKey();//işlem bitene kadar açık kalsın
